a = input("Enter first name: ")
b = input("enter second name: ")
c = list(a)
d = list(b)
for i in range(len(c)):
    for j in range(len(d)):
        if(c[i]==d[j]):
            c[i] = '0'
            d[j] = '0'
print(c)
print(d)
count =0
for i in c:
    if(i!='0'):
        count=count+1
for i in d:
    if(i!='0'):
        count=count+1
print(count)
f=0
res = list("flames")
for i in range(5):
    f = (f+(count-1))%len(res)
    res.pop(f)
print(res[0])
